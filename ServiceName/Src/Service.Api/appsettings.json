{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "TokenOptions": {
    "Audience": "PosCloud",
    "Issuer": "PosCloud",
    "Seconds": 120
  },
  "AllowedHosts": "*",
  "Endpoints": {
    "Mock": "http://localhost:5000/"
  },
  "MessageBus": {
    "Transport": "Rabbit",
    "ConnectionString": "amqp://localhost:9010",
    "Queue": "ServiceName"
  },
  "Database": {
    "ConnectionString": "",
    "Database": "ServiceName"
  },
  "ServiceConfiguration": {
    "KeyName": "ServiceName"
  }
}
